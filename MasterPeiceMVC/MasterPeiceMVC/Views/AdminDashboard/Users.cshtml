        margin: 15px;
        padding: 5px 10px;
        text-align: center;
        font-size: 15px;
        letter-spacing: 1px;
        text-decoration: none;
        color: #0A4438;
        background: transparent;
        cursor: pointer;
        transition: ease-out 0.5s;
        border: 2px solid #0A4438;
        border-radius: 10px;
        box-shadow: inset 0 0 0 0 #0A4438;
    }

        .button:hover {
            color: white;
            box-shadow: inset 0 -100px 0 0 #0A4438;
        }

        .button:active {
            transform: scale(0.9);
        }

